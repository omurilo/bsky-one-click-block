{
  "manifest_version": 3,
  "name": "1-click block bluesky user",
  "description": "A 1-click button to block bluesky user",
  "version": "1.0.3",
  "minimum_chrome_version": "127",
  "host_permissions": ["https://bsky.app/*"],
  "permissions": ["storage"],
  "icons": {
    "128": "icons/icon.jpg"
  },
  "content_scripts": [
    {
      "matches": ["https://bsky.app/*"],
      "js": ["utils.js", "index.js"]
    }
  ]
}
